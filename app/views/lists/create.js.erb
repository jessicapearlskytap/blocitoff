<% unless @list.new_record? %>
	$('#no_lists_msg').hide();
	$('#lists_table').show();
	$('#lists_table tr:last').after("<%= escape_javascript(render 'lists_list_row', list: @list) %>");

	$('#list_name').val('');
	$('#list_permissions').val('<%= List::DEFAULT_PERMISSION %>'); 
<% end %>

$('#new_list').html("<%= escape_javascript(render 'form', list: @list_for_form) %>");
$('#list_name').focus();

<%= render 'shared/toasts', obj: @list %>