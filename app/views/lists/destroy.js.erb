<% if @list.destroyed? %>
  row_id = '#list_<%= @list.id %>';
	$(row_id).fadeOut(400, function() {
		$(row_id).remove();

		if ($('#lists_table .list-list-row').length === 0)
		{
			$('#no_lists_msg').show();
			$('#lists_table').hide();
		}				
	});
<% end %>

<%= render 'shared/toasts', obj: @list %>